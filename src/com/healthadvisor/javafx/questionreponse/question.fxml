<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="paneID" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.healthadvisor.javafx.questionreponse.QuestionController">
   <children>
      <TableView fx:id="tableID" layoutY="-1.0" onMouseClicked="#tableview" prefHeight="363.0" prefWidth="641.0">
        <columns>
            <TableColumn fx:id="utilisateurID" prefWidth="128.0" text="Utilisateur" />
          <TableColumn fx:id="questionID" prefWidth="512.0" text="Question" />
        </columns>
      </TableView>
      <Button fx:id="btnID" layoutX="14.0" layoutY="368.0" mnemonicParsing="false" onAction="#btnPoserQuestionAction" prefHeight="25.0" prefWidth="275.0" text="Poser une question" />
      <Button fx:id="btnConsulter" layoutX="352.0" layoutY="368.0" mnemonicParsing="false" onAction="#btnConsulterQuestion" prefHeight="25.0" prefWidth="275.0" text="Consulter question" />
   </children>
</AnchorPane>
