<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="paneID" prefHeight="600.0" prefWidth="1000.0" stylesheets="@FlatBee.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.healthadvisor.javafx.questionreponse.QuestionController">
   <children>
      <TableView fx:id="tableID" layoutY="54.0" onMouseClicked="#tableview" prefHeight="444.0" prefWidth="1000.0">
        <columns>
            <TableColumn fx:id="utilisateurID" minWidth="0.0" prefWidth="117.0" text="Utilisateur" />
          <TableColumn fx:id="questionID" minWidth="0.0" prefWidth="202.0" text="Question" />
            <TableColumn fx:id="dateID" maxWidth="467.0" minWidth="-1.0" prefWidth="456.0" text="Date de publication" />
            <TableColumn fx:id="specialite" maxWidth="464.0" minWidth="-1.0" prefWidth="224.0" text="Spécialité" />
        </columns>
      </TableView>
      <Button fx:id="btnConsulter" layoutX="390.0" layoutY="528.0" mnemonicParsing="false" onAction="#btnConsulterQuestion" prefHeight="27.0" prefWidth="153.0" text="Consulter question" />
      <TextField fx:id="searchBarID" layoutX="809.0" layoutY="14.0" promptText="Chercher une question ..." />
   </children>
</AnchorPane>
